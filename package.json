{
  "name": "module-from-string",
  "version": "2.0.0",
  "description": "Load module from string, require and import.",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "directories": {
    "test": "__tests__"
  },
  "scripts": {
    "clean": "rimraf .cache dist",
    "dts": "tsc -d --emitDeclarationOnly --declarationDir .cache",
    "bundle": "rollup -c",
    "build": "yarn clean && yarn dts && yarn bundle",
    "lint": "ts-standardx",
    "lint:fix": "ts-standardx --fix",
    "test": "yarn lint && yarn build && jest --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/exuanbo/module-from-string.git"
  },
  "keywords": [
    "load",
    "module",
    "string",
    "require",
    "require-from-string",
    "import",
    "vm",
    "esbuild"
  ],
  "author": "exuanbo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/exuanbo/module-from-string/issues"
  },
  "homepage": "https://github.com/exuanbo/module-from-string#readme",
  "dependencies": {
    "esbuild": "0.8.15"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "6.1.0",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.20",
    "jest": "26.6.3",
    "rollup": "2.38.5",
    "rollup-plugin-dts": "2.0.1",
    "ts-jest": "26.5.0",
    "ts-standardx": "0.5.0",
    "tslib": "2.0.3",
    "typescript": "4.1.2"
  }
}
